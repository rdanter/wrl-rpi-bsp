Subject: [PATCH] overlays: Mark more overlays as Pi4-specific

Signed-off-by: Phil Elwell <phil@raspberrypi.com>
---
 arch/arm/boot/dts/overlays/overlay_map.dts | 20 ++++++++++++++++++++
 1 file changed, 20 insertions(+)

diff --git a/arch/arm/boot/dts/overlays/overlay_map.dts b/arch/arm/boot/dts/overlays/overlay_map.dts
index 3944b0c30986..df63a1793b42 100644
--- a/arch/arm/boot/dts/overlays/overlay_map.dts
+++ b/arch/arm/boot/dts/overlays/overlay_map.dts
@@ -41,6 +41,22 @@ lirc-rpi {
 		deprecated = "use gpio-ir";
 	};
 
+	midi-uart2 {
+		bcm2711;
+	};
+
+	midi-uart3 {
+		bcm2711;
+	};
+
+	midi-uart4 {
+		bcm2711;
+	};
+
+	midi-uart5 {
+		bcm2711;
+	};
+
 	pcie-32bit-dma {
 		bcm2711;
 	};
@@ -172,4 +188,8 @@ vc4-kms-v3d {
 	vc4-kms-v3d-pi4 {
 		bcm2711;
 	};
+
+	vl805 {
+		bcm2711;
+	};
 };
