From f181d773661bcbdd162c8eac283fbf93fd21deda Mon Sep 17 00:00:00 2001
From: Phil Elwell <phil@raspberrypi.org>
Date: Mon, 26 Nov 2018 21:01:18 +0000
Subject: [PATCH 096/587] Revert "build/arm64: Add rules for .dtbo files for
 dts overlays"

DT build rules are now in the common top-level Makefile.

This reverts commit dce5b0fbdd2174f7fc9b27f4e89877a33169239a.
---
 arch/arm64/Makefile | 14 --------------
 1 file changed, 14 deletions(-)

diff --git a/arch/arm64/Makefile b/arch/arm64/Makefile
index 698cbb46a61d..e9d2e578cbe6 100644
--- a/arch/arm64/Makefile
+++ b/arch/arm64/Makefile
@@ -146,20 +146,6 @@ Image.%: Image
 zinstall install:
 	$(Q)$(MAKE) $(build)=$(boot) $@
 
-%.dtb: scripts
-	$(Q)$(MAKE) $(build)=$(boot)/dts $(boot)/dts/$@
-
-%.dtbo: | scripts
-	$(Q)$(MAKE) $(build)=$(boot)/dts MACHINE=$(MACHINE) $(boot)/dts/$@
-
-PHONY += dtbs dtbs_install
-
-dtbs: prepare scripts
-	$(Q)$(MAKE) $(build)=$(boot)/dts
-
-dtbs_install:
-	$(Q)$(MAKE) $(dtbinst)=$(boot)/dts
-
 PHONY += vdso_install
 vdso_install:
 	$(Q)$(MAKE) $(build)=arch/arm64/kernel/vdso $@
-- 
